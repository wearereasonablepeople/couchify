{
  "name": "couchify",
  "description": "use next generation JS in your CouchDB apps",
  "version": "0.4.0",
  "main": "lib/index.js",
  "author": "wearereasonablepeople",
  "license": "MIT",
  "scripts": {
    "build": "rimraf lib && tsc",
    "lint": "tslint src/**/*.ts --project tsconfig.json --type-check",
    "pretest": "npm run build",
    "test": "tape lib/test/**/*.js | tap-diff",
    "release": "xyz --edit --repo git@github.com:wearereasonablepeople/couchify.git --increment"
  },
  "bin": {
    "couchify": "cmd.js"
  },
  "typings": "lib/index",
  "repository": {
    "type": "git",
    "url": "https://github.com/wearereasonablepeople/couchify.git"
  },
  "dependencies": {
    "acorn": "^5.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^7.3.0",
    "deps-sort": "^2.0.0",
    "glob": "^7.1.2",
    "mime-types": "^2.1.16",
    "minimist": "^1.2.0",
    "module-deps": "^4.1.1",
    "node-couchdb": "^1.2.0",
    "pump": "^1.0.2",
    "stream-to-array": "^2.3.0",
    "toposort": "^1.0.3",
    "transform-deps": "^2.0.0",
    "uniq": "^1.0.1",
    "xyz": "^2.1.0"
  },
  "devDependencies": {
    "@types/acorn": "^4.0.2",
    "@types/glob": "^5.0.32",
    "@types/mime-types": "^2.1.0",
    "@types/minimist": "^1.2.0",
    "@types/node": "^8.0.24",
    "@types/pump": "^1.0.1",
    "@types/tape": "^4.2.30",
    "@types/uniq": "^0.0.27",
    "babel-plugin-transform-function-bind": "^6.22.0",
    "couchdb-client": "^1.0.14",
    "gamma": "^1.0.0",
    "rimraf": "^2.6.1",
    "tap-diff": "^0.1.1",
    "tape": "^4.8.0",
    "tslint": "^5.6.0",
    "tslint-eslint-rules": "^4.1.1",
    "typescript": "^2.4.2"
  },
  "files": [
    "lib",
    "cmd.js"
  ]
}
